module KeywordExpression

imports
	Expression
	Method
	OperatorExpression

context-free syntax

	KeywordExp.KeyNot = KeywordNOTExp
	KeywordExp.KeyOr = KeywordORExp
	KeywordExp.KeyAnd = KeywordANDExp

	KeywordNOTExp.Method = <<MethodInvocationWithoutParentheses>>
	KeywordNOTExp.NotMethod = <!<MethodInvocationWithoutParentheses>>	{right}
	KeywordNOTExp.OpExp = <<OpExp>>
	KeywordNOTExp.Not = <not <KeywordNOTExp>>							{right}
	
	KeywordORExp.Or = <<Exp> or <KeywordORExp>>							{left}
	
	KeywordANDExp.And = <<Exp> and <KeywordNOTExp>>						{left}