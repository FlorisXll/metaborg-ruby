module HashConstructor

imports
  OperatorExpression

context-free syntax
  HashConstructor.HashConstructor = <{ <HashAssociationList?> }>
  
  HashAssociationList.HashAssocList = <<AssociationList> <HashSep?>>
  
  HashSep.FinalComma = ","

  AssociationList.AssocList = {Association ","}*
  AssociationListNonEmpty.AssocList = {Association ","}+
  
  Association.Assoc = [[AssociationKey] => [AssociationValue]]
  
  AssociationKey.AssocKey = OpExp
  
  AssociationValue.AssocValue = OpExp
  