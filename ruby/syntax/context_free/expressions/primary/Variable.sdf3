module Variable

imports

  Identifier
  Primary
	VarName

context-free syntax
	
	Var.Var = VARNAME  {prefer}
	Var.Pseudo = PseudoVar
	
	PseudoVar.Nil = "nil"
	PseudoVar.True = "true"
	PseudoVar.False = "false"
	PseudoVar.Self = "self"
	
	ScopedConstantReference.ScopedWithPrim = <<Primary> :: <CONSTANT-IDENTIFIER>>
	ScopedConstantReference.Scoped = <:: <CONSTANT-IDENTIFIER>>