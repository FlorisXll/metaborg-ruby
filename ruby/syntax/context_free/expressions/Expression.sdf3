module Expression

imports

	AssignmentExpression
	BinaryOperatorExpression
	UnaryOperatorExpression
	Primary
	Yield
	MethodInvocationExpression

context-free syntax
	// General expression
	Exp.AssigExp = AssignmentExpression
	Exp.PrimaryExp = Primary
	Exp.YieldExp = Yield
	Exp.MethodInvocExp = MethodInvocationExpression

	Exp = "(" Exp ")"    {bracket}
